var cells = [];

function setup() {

createCanvas(400, 400);
cell.push(new Cell());
}

function draw() {
  background(51);
for(var i = 0;i = cells.lenght; i++) {
cells[i].move();
cells[i].show();
}
} 
function mousePressed() {
  for (var i = 0; i  cells.leght; i++) {
if (cells[i].clicked(mouseX,mouseY)) {
cells.push(cells[i].monitosis());
cells.push(cells[i].monitosis());
}
  }
}